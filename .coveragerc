[run]
source = app
omit =
    tests/*
    app/config/*
    */migrations/*
    */__pycache__/*
    */site-packages/*
    */python?.?/*
    */venv/*
    */.venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    if TYPE_CHECKING:
